  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["${command:pickArgs}"]
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug none_tests.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test/none_tests.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug few_tests.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test/few_tests.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug many_tests.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test/many_tests.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug some_tests.py",
